<!-- Preloader-->
<div class="page-loader">
    <div class="loader">Loading...</div>
</div>
<!-- Preloader end-->
<%= render "partials/menu" %>
<div class="wrapper">
    <!-- Hero section-->
    <section class="split-image parallax portfolio-img" >
        <div class="split-image-footer">
            <ul class="split-list">
                <li><%= link_to "Download Resume", download_pdf_path %></li>
            </ul>
        </div>
    </section>
    <!-- Hero section end-->
    <!-- Content section-->
    <section class="split-content">
        <!-- Page content-->
        <section class="module">
            <div class="container-fluid container-custom">
                <div class="row">
                    <div class="col-sm-12">
                        <article class="post">
                            <div class="post-header">
                                <h1 class="post-title">Latest Works &amp; Projects</h1>
                                <ul class="post-meta">
                                    <li><span>2017 | 2018</span></li>
                                </ul>
                            </div>
                            <!--  Project #1 -->
                            <%=link_to("https://vegissimo.herokuapp.com/") do %>
                            <div class="post-content portfolio-item brand">
                                <div class="portfolio-wrapper"><%= image_tag("vegissimo1.jpg") %>
                                    <div class="portfolio-overlay"></div>
                                    <div class="portfolio-caption">
                                        <h6 class="portfolio-title">Vegissimo</h6><span class="portfolio-subtitle">Recipe Search Engine</span>
                                    </div>
                                </div>
                            </div>
                            <% end %>
                            <!-- Project #1 End -->
                            <!--  Project #2 -->
                            <%=link_to("https://camelot-chess.herokuapp.com") do %>
                            <div class="post-content portfolio-item brand">
                                <div class="portfolio-wrapper"><%= image_tag("camelotchess2.JPG") %>
                                    <div class="portfolio-overlay"></div>
                                    <div class="portfolio-caption">
                                        <h6 class="portfolio-title">Camelot Chess</h6><span class="portfolio-subtitle">Ruby On Rails Chess</span>
                                    </div>
                                </div>
                            </div>
                            <% end %>
                            <!-- Project #2 End -->
                            <!--  Project #3 -->
                            <%=link_to("https://flixter-belder.herokuapp.com/") do %>
                            <div class="post-content portfolio-item brand">
                                <div class="portfolio-wrapper"><%= image_tag("flixter1.JPG") %>
                                    <div class="portfolio-overlay"></div>
                                    <div class="portfolio-caption">
                                        <h6 class="portfolio-title">Flixter</h6><span class="portfolio-subtitle">A Two-sided Video Streaming Marketplace</span>
                                    </div>
                                </div>
                            </div>
                            <% end %>
                            <!-- Project #3 End -->
                            <!--  Project #4 -->
                            <%=link_to("https://grammable-belder.herokuapp.com/") do %>
                            <div class="post-content portfolio-item brand">
                                <div class="portfolio-wrapper p2img"><%= image_tag("grammable1.JPG") %>
                                    <div class="portfolio-overlay"></div>
                                    <div class="portfolio-caption">
                                        <h6 class="portfolio-title">Grammable</h6><span class="portfolio-subtitle">Instagram Clone</span>
                                    </div>
                                </div>
                            </div>
                            <% end %>
                            <!-- Project #4 End -->
                        </article>
                    </div>
                </div>
            </div>
        </section>
        <!-- Page content end-->
        <%= render 'partials/footer' %>
    </section>
    <!-- Content section end-->
</div>